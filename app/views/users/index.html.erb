<h1>Listing users</h1>

<table class="scaffold-table">
  <%= render partial: 'user_headings' %>


  <tbody>
    <% @users.each do |user| %>
      <%= render partial: 'user_details', :locals => { :user => user } %>
    <% end %>
  </tbody>
</table>

<% if can? :manage, User %>
    <li><%= link_to 'Manage Users', users_path %></li>
<% end %>

